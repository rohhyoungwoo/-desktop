package print;
//8번 : 출력메소드(printf)

public class PrintTest02 {
	public static void main(String[] args) {
		//정수형
		int num1 = 200;
		System.out.printf("10진수 : %d", 100); //10진수 값 출력
		System.out.println();
		System.out.printf("10진수 : %d", num1); //10진수 값 출력
		System.out.printf("\n8진수 : %o", 0377);
	}

}
