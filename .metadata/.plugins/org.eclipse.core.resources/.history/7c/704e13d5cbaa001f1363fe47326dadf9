package vauable;
//1번
public class Vauavle01 {
	public static void main(String[] args) {
		//변수 선언
		//자료형 변수명;
		int number1;
		boolean isTrue;
		double number2;
		
		//변수의 값 저장
		//변수명 = 값;
		number = 10;
		number = 10.5;
		
		System.out.println(10);
		System.out.println(number1);
		System.out.println(number2);
	}

}
