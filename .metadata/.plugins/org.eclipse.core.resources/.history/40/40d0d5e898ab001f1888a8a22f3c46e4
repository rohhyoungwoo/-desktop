package scanner;
//3번: next()와 nextLine()

import java.util.Scanner;

public class ScannerTest03 {
	public static void main(String[] args) {
		
		//1. 입력클래스 import
		Scanner sc = new Scanner(System.in);
		
		System.out.println("입력 : ");
//		System.out.println(sc.next());
//		System.out.println(sc.next());
//		System.out.println(sc.next());
		
		System.out.println(sc.nextLine());
		System.out.println(sc.nextLine());
		System.out.println(sc.nextLine());
	}
}
