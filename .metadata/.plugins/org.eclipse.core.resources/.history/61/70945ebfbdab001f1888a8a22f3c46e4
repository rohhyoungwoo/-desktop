package oper;
//9번 : 복합대입 연산자

public class OperTest02 {
	public static void main(String[] args) {
		
		int num = 1;
		System.out.println(num);
		System.out.println(num + 10);
		
//		num = num + 10;
		System.out.println(num);
		num += 10;
		//num에 저장된 값에 + 10 연산된 값을 num 변수에 저장한다
		System.out.println(num);
		
		num *= 2;
		System.out.println(num);
		
		num /= 2;
		System.out.println(num);
		
		
		
		
	}
}
